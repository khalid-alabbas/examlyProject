<!-- DOM elements (html) -->
<link rel="stylesheet" href="../css/partials/sidebar.css">
<!-- this recives user variable which can be admin, quest, student -->
<div class="side-bar ">
        <h1 id="examly-logo">
                <img src="data:image/png;base64," />
                        Examly</h1>
<hr>
        <div class="main-menu">
                <!-- Explore button -->
                <div id="explore-button" class="menu-element">
                        <img src="data:image/png;base64,"/>
                        Explore
                </div>
                
                <!-- add my courses and history buttons only for student -->
                <% if (user=='student') { %> 
                        <div id="myCourses-button" class="menu-element">
                                <img src="data:image/png;base64," />
                                My courses
                        </div>
                        <!-- History -->
                        <div id="history-button" class="menu-element">
                                <img src="data:image/png;base64," />
                                History
                        </div>
                <% } %>

                <!-- add requests and users buttons only for admin -->              
                <% if (user=='admin' ) { %>
                        <div id="requests-button" class="menu-element">
                                <img src="data:image/png;base64," />
                                Requests
                        </div>
                        <!-- History -->
                        <div id="users-button" class="menu-element">
                                <img src="data:image/png;base64," />
                                Users
                        </div>
                        <% } %>
                
                <!-- Settings -->
                <div class="menu-element">
                        <img src="data:image/png;base64," />
                        Settings
                </div>
                <!-- About us -->
                <div class="menu-element">
                <img src="data:image/png;base64," /> 
                        About Us
                </div>

                


        </div>

<hr>
        <% if (user=='quest'){ %> 
                <button>SIGN IN</button>
        <% }else{ %>
                <button>UPLOAD</button>
        <% } %> 


</div>